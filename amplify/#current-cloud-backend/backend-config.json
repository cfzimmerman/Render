{
  "function": {
    "webstagingauthCreateAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "webstagingauthDefineAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "webstagingauthPreSignup": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "webstagingauthVerifyAuthChallengeResponse": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "auth": {
    "webstagingauth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "webstagingauthCreateAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "webstagingauthDefineAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "webstagingauthPreSignup",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "webstagingauthVerifyAuthChallengeResponse",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": true,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "PHONE_NUMBER"
        ]
      }
    }
  }
}